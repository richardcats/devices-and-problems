<Window x:Class="DevicesEnStoringen.Overzicht"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DevicesEnStoringen"
        mc:Ignorable="d"
        Title="Alle storingen" Height="350" Width="600">
    <Grid>
        <Canvas x:Name="cvsNavigatie" Margin="10,10,0,0">
            <Button Content="Storingen" Width="75" Click="StoringenClick" />
            <Button Content="Devices" Width="75" Canvas.Left="100" Click="DevicesClick"/>
            <Button Content="Device-types" Width="75" Canvas.Left="200" Click="DeviceTypesClick"/>
            <Button Content="Rapportages" Width="75" Canvas.Left="300" Click="RapportagesClick" />
            <Separator  Height="54"  Width="574"/>
        </Canvas>
        <StackPanel Name="stkOverzicht"/>
    </Grid>
</Window>
